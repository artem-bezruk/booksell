    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "^9.1.7",
    "@angular/language-service": "^9.1.7",
    "@types/hammerjs": "^2.0.36",
    "@types/jest": "^26.0.4",
    "@types/node": "^13.9.0",
    "codelyzer": "^5.1.2",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "sonarqube-scanner": "^2.5.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
