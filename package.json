    "build-ci": "ng build --prod --progress=false --output-path=dist",
    "test": "jest",
    "test-ci": "jest --coverage",
    "test-watch": "jest --watch",
    "publish-test-coverage": "cat ./coverage/lcov.info | codacy-coverage",
